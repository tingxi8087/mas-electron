{
  "name": "mas-electron",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/electron/main.js",
  "scripts": {
    "start": "concurrently \"npm run dev:web\" \"npm run dev:electron\"",
    "dev:web": "vite",
    "dev:electron": "nodemon",
    "build": "npm run buildFile && electron-builder",
    "build:mac": "npm run buildFile && electron-builder --mac",
    "build:win": "npm run buildFile && electron-builder --win",
    "compile:electron": "tsc -p tsconfig.electron.json",
    "compile:web": "vite build",
    "minify:electron": "node scripts/minify-electron.js",
    "buildFile": "npm run compile:web && npm run compile:electron && npm run minify:electron "
  },
  "dependencies": {},
  "devDependencies": {
    "@ant-design/icons": "^5.2.6",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "antd": "^5.6.0",
    "axios": "^1.4.0",
    "concurrently": "^9.2.1",
    "e-boxes": "^0.0.2",
    "electron": "^40.0.0",
    "electron-builder": "^26.4.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "less": "^4.1.3",
    "mas-encrypt": "^1.0.2",
    "nodemon": "^3.1.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.11.2",
    "tailwindcss": "^4.1.18",
    "terser": "^5.46.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.9"
  },
  "build": {
    "appId": "com.mas-electron.app",
    "productName": "mas-electron",
    "directories": {
      "output": "build"
    },
    "compression": "maximum",
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
